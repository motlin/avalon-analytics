{
	"name": "@redwoodjs/starter-standard",
	"version": "1.0.0",
	"description": "Build fast, server-driven webapps on Cloudflare with SSR, RSC, and realtime.",
	"main": "index.js",
	"type": "module",
	"private": true,
	"scripts": {
		"prebuild": "prisma generate && mkdir -p .wrangler/state/v3/d1",
		"build": "vite build",
		"postbuild": "cp ./dist/client/assets/query_engine_bg*.wasm dist/worker/assets/ 2>/dev/null || true && cp ./dist/worker/assets/worker-entry.css dist/client/assets/ 2>/dev/null || true",
		"dev": "vite dev",
		"dev:init": "rw-scripts dev-init",
		"preview": "vite preview",
		"worker:run": "rw-scripts worker-run",
		"clean": "rm -rf dist .wrangler node_modules/.vite node_modules/.cache",
		"release": "rw-scripts ensure-deploy-env && pnpm run clean && pnpm run build && wrangler deploy",
		"migrate:dev": "prisma generate && wrangler d1 migrations apply DB --local",
		"migrate:prd": "wrangler d1 migrations apply DB --remote",
		"populate-people": "pnpm exec tsx src/scripts/populate-people.ts",
		"deploy:cf": "wrangler deploy --config dist/worker/wrangler.json && pnpm run migrate:prd",
		"migrate:new": "rw-scripts migrate-new",
		"seed": "pnpm run worker:run ./src/scripts/seed.ts",
		"generate": "rw-scripts ensure-env && prisma generate && wrangler types",
		"check": "pnpm run generate && pnpm run types",
		"types": "tsc",
		"prepare": "vibe-rules install cursor",
		"ci:eslint": "eslint . --format json --output-file eslint_report.json",
		"format": "biome format --write . && prettier --write .",
		"ci:format": "biome format . && prettier --check .",
		"ci:biome": "biome format .",
		"ci:typecheck": "tsc --noEmit",
		"test": "vitest",
		"test:run": "vitest run",
		"storybook": "storybook dev -p 6006",
		"storybook:test": "vitest run --config vitest.storybook.config.ts",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.0",
		"@fortawesome/fontawesome-svg-core": "^6.7.2",
		"@fortawesome/free-brands-svg-icons": "^6.7.2",
		"@fortawesome/free-regular-svg-icons": "^6.7.2",
		"@fortawesome/free-solid-svg-icons": "^6.7.2",
		"@fortawesome/react-fontawesome": "^0.2.3",
		"@mui/icons-material": "^7.1.0",
		"@mui/material": "^7.1.0",
		"@prisma/adapter-d1": "^6.8.2",
		"@prisma/client": "^6.8.2",
		"@simplewebauthn/browser": "^13.1.0",
		"@simplewebauthn/server": "^13.1.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"firebase": "^11.10.0",
		"json5": "^2.2.3",
		"lucide-react": "^0.513.0",
		"markdown-to-jsx": "^7.7.7",
		"react": "19.3.0-canary-fb2177c1-20251114",
		"react-dom": "19.3.0-canary-fb2177c1-20251114",
		"react-server-dom-webpack": "19.3.0-canary-fb2177c1-20251114",
		"rwsdk": "1.0.0-beta.39",
		"tailwind-merge": "^3.4.0",
		"zod": "^4.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@cloudflare/vite-plugin": "1.17.0",
		"@cloudflare/vitest-pool-workers": "^0.8.53",
		"@cloudflare/workers-types": "4.20251209.0",
		"@eslint/js": "^9.30.0",
		"@storybook/addon-docs": "^9.1.8",
		"@storybook/addon-onboarding": "^9.1.8",
		"@storybook/addon-vitest": "^9.1.6",
		"@storybook/react": "^9.1.8",
		"@storybook/react-vite": "^9.1.8",
		"@tailwindcss/postcss": "^4.1.17",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "~24.5.2",
		"@types/react": "19.1.2",
		"@types/react-dom": "19.1.2",
		"@typescript-eslint/eslint-plugin": "^8.36.0",
		"@typescript-eslint/parser": "^8.36.0",
		"@vitest/browser": "^3.2.4",
		"autoprefixer": "^10.4.22",
		"eslint": "^9.39.1",
		"eslint-plugin-storybook": "^9.1.8",
		"globals": "^16.3.0",
		"jiti": "^2.1.5",
		"miniflare": "^3.20250408.2",
		"playwright": "^1.54.1",
		"prettier": "^3.7.4",
		"prisma": "^6.8.2",
		"storybook": "^9.1.8",
		"tailwindcss": "^4.1.17",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vibe-rules": "^0.2.31",
		"vite": "7.2.4",
		"vitest": "^3.2.4",
		"wrangler": "4.53.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@prisma/client",
			"@prisma/engines",
			"esbuild",
			"prisma",
			"sharp",
			"workerd"
		]
	}
}
