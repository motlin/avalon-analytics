{
	"name": "@redwoodjs/starter-standard",
	"version": "1.0.0",
	"description": "Build fast, server-driven webapps on Cloudflare with SSR, RSC, and realtime. ",
	"main": "index.js",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "vite build",
		"dev": "wrangler dev",
		"dev:vite": "NODE_ENV=${NODE_ENV:-development} vite dev",
		"dev:init": "rw-scripts dev-init",
		"preview": "vite preview",
		"worker:run": "rw-scripts worker-run",
		"clean": "rm -rf dist .wrangler node_modules/.vite node_modules/.cache",
		"release": "rw-scripts ensure-deploy-env && npm run clean && prisma generate && RWSDK_DEPLOY=1 npm run build && wrangler deploy",
		"migrate:dev": "prisma generate && wrangler d1 migrations apply DB --local",
		"migrate:prd": "wrangler d1 migrations apply DB --remote",
		"migrate:new": "rw-scripts migrate-new",
		"seed": "npm run worker:run ./src/scripts/seed.ts",
		"generate": "rw-scripts ensure-env && prisma generate && wrangler types",
		"check": "npm run generate && npm run types",
		"types": "tsc",
		"prepare": "vibe-rules install cursor",
		"ci:eslint": "eslint . --format json --output-file eslint_report.json",
		"format": "biome format --write . && prettier --write .",
		"ci:format": "biome format . && prettier --check .",
		"ci:biome": "biome format .",
		"ci:typecheck": "tsc --noEmit",
		"test": "vitest",
		"test:run": "vitest run",
		"storybook": "storybook dev -p 6006",
		"storybook:test": "vitest run --config vitest.storybook.config.ts",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"@prisma/adapter-d1": "~6.16.2",
		"@prisma/client": "~6.16.2",
		"@simplewebauthn/browser": "^13.2.0",
		"@simplewebauthn/server": "^13.2.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"firebase": "^12.3.0",
		"json5": "^2.2.3",
		"lucide-react": "^0.544.0",
		"rwsdk": "0.3.12",
		"tailwind-merge": "^3.3.1",
		"zod": "^4.1.11"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@cloudflare/vite-plugin": "1.13.2",
		"@cloudflare/vitest-pool-workers": "^0.9.5",
		"@cloudflare/workers-types": "^4.20250924.0",
		"@eslint/js": "^9.36.0",
		"@storybook/addon-docs": "^9.1.6",
		"@storybook/addon-onboarding": "^9.1.8",
		"@storybook/addon-vitest": "^9.1.6",
		"@storybook/react": "^9.1.8",
		"@storybook/react-vite": "^9.1.8",
		"@tailwindcss/postcss": "^4.1.13",
		"@tailwindcss/vite": "^4.1.13",
		"@types/node": "^24.5.2",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"@typescript-eslint/eslint-plugin": "^8.44.1",
		"@typescript-eslint/parser": "^8.44.1",
		"@vitest/browser": "^3.2.4",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.36.0",
		"eslint-plugin-storybook": "^9.1.6",
		"globals": "^16.4.0",
		"jiti": "^2.5.1",
		"miniflare": "^4.20250924.0",
		"prettier": "^3.6.2",
		"prisma": "~6.16.2",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"storybook": "^9.1.8",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2",
		"vibe-rules": "^0.3.91",
		"vite": "^6.3.6",
		"vitest": "^3.2.4",
		"wrangler": "^4.40.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@prisma/client",
			"@prisma/engines",
			"esbuild",
			"prisma",
			"sharp",
			"workerd"
		]
	}
}
